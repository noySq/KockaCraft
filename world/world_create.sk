function world_create(world: text, env: environment):
	
	if {_world} is loaded:
		server_log("world", yaml value "messages.already-exist" from "world-config")
		stop
	
	set {_w} to world creator named {_world}

	set environment option of {_w} to {_env}
	load world from creator {_w}
	
	server_log("world", yaml value "messages.successfully-created" from "world-config")