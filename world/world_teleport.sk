function world_teleport(p: player, world: text):

	if {_world} is not loaded:
		send server_feedback("error", yaml value "messages.not-exist" from "world-config") to {_p}
		stop
		
	teleport {_p} to spawn of world({_world})
	send server_feedback("casual", yaml value "messages.teleporting" from "world-config") to {_p}